{
  "name": "padel-boy",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "type-check": "tsc --noEmit",
    "test": "vitest run --no-cache",
    "test:watch": "vitest",
    "translate:check": "npm run type-check && npm run test",
    "generate:translation": "tsx src/locales/scripts/generate-translation.ts",
    "build": "npm run type-check && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "i18next": "^25.8.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.563.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.71.1",
    "react-i18next": "^16.5.4",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0",
    "sonner": "^2.0.7",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.2.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/ui": "^2.1.9",
    "eslint": "^9.13.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "jsdom": "^28.0.0",
    "postcss": "^8.5.6",
    "simple-git-hooks": "^2.13.1",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.9"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run translate:check"
  }
}
